{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Cool Browser",
  "description": "Browse the web privatly",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "1.2",
  "default_locale": "en",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["config.js", "main.js"]
    }
  },
  "permissions": [
    "webview"
  ]
}